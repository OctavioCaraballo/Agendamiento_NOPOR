@page
@model AgendamientoIPS.App.Frontend.Pages.Medicos.CreateModel
@{
}
<h1>Nuevo Médico</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="medico.Nombre"></label>
        <input asp-for="medico.Nombre" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="medico.PrimerApellido">Primer Apellido</label>
        <input asp-for="medico.PrimerApellido" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="medico.SegundoApellido">Segundo Apellido</label>
        <input asp-for="medico.SegundoApellido" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="medico.Direccion"></label>
        <input asp-for="medico.Direccion" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="medico.Telefono"></label>
        <input asp-for="medico.Telefono" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="medico.Correo"></label>
        <input asp-for="medico.Correo" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="medico.TarjetaProfesional">Tarjeta Profesional</label>
        <input asp-for="medico.TarjetaProfesional" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="medico.Especialidad"></label>
        <input asp-for="medico.Especialidad" class="form-control">
    </div>
    <button type="submit" class="btn btn-primary">Crear Medico</button>
</form>
<a class="btn btn-secondary" asp-page="./Index">Volver</a>