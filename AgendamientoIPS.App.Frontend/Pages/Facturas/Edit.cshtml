@page
@model AgendamientoIPS.App.Frontend.Pages.Facturas.EditModel
@{
}
<h1>Editar Factura</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="factura.Id"></label>
        <input asp-for="factura.Id" class="form-control" readonly>
    </div>        
    <div class="form-group">
        <label asp-for="factura.NumFactura">Número Factura</label>
        <input asp-for="factura.NumFactura" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="factura.FechaFactura">Fecha Factura</label>
        <input asp-for="factura.FechaFactura" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="factura.Concepto">Concepto</label>
        <input asp-for="factura.Concepto" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="factura.TarifaAplicada">Tarifa Aplicada</label>
        <input asp-for="factura.TarifaAplicada" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="factura.ValorPagado">Valor Pagado</label>
        <input asp-for="factura.ValorPagado" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="factura.FormadePago">Forma de Pago</label>
        <input asp-for="factura.FormadePago" class="form-control">
    </div>
    <button type="submit" class="btn btn-primary">Editar Factura</button>
</form>
<a class="btn btn-secondary" asp-page="./Index">Volver</a>
